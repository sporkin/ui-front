{
  "name": "ui-fron",
  "version": "0.0.1",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "express": "3.0.3",
    "jade": "*",
    "redis": "~0.7.2",
    "hiredis": "~0.1.14",
    "coffee-script": "~1.3.3",
    "connect-redis": "~1.4.0",
    "express-namespace": "~0.1.1",
    "underscore": "~1.3.3",
    "stylus": "~0.28.2",
    "qs": "~0.5.0",
    "socket.io": "~0.9.6",
    "nib": "~0.9.0",
    "dustjs-linkedin": "~1.1.1",
    "consolidate": "~0.7.0",
    "dustjs-helpers": "~1.1.0"
  },
  "devDependencies": {
    "node-dev": "~0.2.4",
    "mocha": "~1.3.0",
    "chai": "*",
    "sinon-chai": "~2.3.0",
    "request": "~2.9.203",
    "libxmljs": "~0.5.4"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "start": "node server.js",
    "watch": "./node_modules/.bin/mocha --growl --watch",
    "devserver": "./node_modules/node-dev/node-dev server.js"
  },
  "jam": {
    "packageDir": "public/javascripts/vendor",
    "baseUrl": "public/javascripts",
    "dependencies": {
      "jquery": null,
      "backbone": null,
      "underscore": null
    },
    "include": [
      "models/user.js"
    ],
    "config": {
      "paths": {
        "bootstrap": [
          "//netdna.bootstrapcdn.com/twitter-bootstrap/2.2.1/js/bootstrap.min",
          "libs/bootstrap"
        ]
      }
    },
    "shim": {
      "backbone": {
        "deps": [
          "jquery",
          "underscore"
        ],
        "exports": "Backbone"
      },
      "underscore": {
        "exports": "_"
      },
      "bootstrap": [
        "jquery"
      ]
    }
  }
}